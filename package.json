    "redux-watch": "^1.2.0",
    "simplebar-react": "^2.3.0",
    "urijs": "^1.19.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
